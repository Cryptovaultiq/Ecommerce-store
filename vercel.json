{
  "routes": [
    { "src": "/", "dest": "/customer.html" },        // Default to customer page
    { "src": "/admin", "dest": "/index.html" },      // Admin page at /admin
    { "src": "/shop", "dest": "/customer.html" },    // Optional alias for shop
    { "src": "/(.*)", "dest": "/customer.html" }     // Catch-all for other paths
  ]
}
